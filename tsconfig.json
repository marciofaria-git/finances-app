{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "declaration": true,
    "emitDeclarationOnly": false,
    "outDir": "dist",
    "declarationMap": true,
  },
  "include": ["src","next-env.d.ts", "desc.d.ts","**/*.ts", "**/*.tsx", "src/pages/_document.jsx", "node_modules/jest-changed-files/desc.d.ts"],
  "exclude": ["node_modules"]
}